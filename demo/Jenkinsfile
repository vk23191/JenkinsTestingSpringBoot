pipeline {
    agent any
    
    tools {
        // Use the Git installation named 'Default' configured in Jenkins
        git 'Default'
    }

    stages {
        stage('Checkout') {
            steps {
                // Checkout the Git repository
                git branch: 'main', url: 'https://github.com/vk23191/JenkinsTestingSpringBoot.git'
            }
        }
        // Add more stages as needed
    }
}
